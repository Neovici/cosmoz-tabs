{
	"name": "@neovici/cosmoz-tabs",
	"version": "5.1.1",
	"description": "A multi views container element that allow navigation between the views using tabs or an accordion.",
	"keywords": [
		"web-components"
	],
	"homepage": "https://github.com/neovici/cosmoz-tabs#readme",
	"bugs": {
		"url": "https://github.com/neovici/cosmoz-tabs/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/neovici/cosmoz-tabs.git"
	},
	"license": "Apache-2.0",
	"author": "Neovici Development <dev@neovici.se>",
	"main": "cosmoz-tabs.js",
	"directories": {
		"test": "test"
	},
	"files": [
		"cosmoz-*.js",
		"lib/**/*.js"
	],
	"scripts": {
		"lint": "eslint --cache --ext .js .",
		"lint-tsc": "tsc",
		"start": "npm run storybook",
		"test": "wtr --coverage",
		"test:watch": "wtr --watch",
		"storybook": "start-storybook --node-resolve --watch --open",
		"storybook:build": "build-storybook",
		"storybook:deploy": "storybook-to-ghpages",
		"prepare": "husky install"
	},
	"release": {
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			"@semantic-release/changelog",
			"@semantic-release/github",
			"@semantic-release/npm",
			"@semantic-release/git"
		],
		"branch": "master"
	},
	"publishConfig": {
		"access": "public"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"dependencies": {
		"@neovici/cosmoz-utils": "^3.21.0",
		"@polymer/iron-icon": "^3.0.0",
		"@polymer/iron-icons": "^3.0.0",
		"haunted": "^4.8.0",
		"lit-html": "^1.4.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^13.0.0",
		"@commitlint/config-conventional": "^13.0.0",
		"@neovici/eslint-config": "^1.3.0",
		"@open-wc/demoing-storybook": "^2.4.0",
		"@open-wc/testing": "^2.5.0",
		"@polymer/iron-list": "^3.1.0",
		"@semantic-release/changelog": "^6.0.0",
		"@semantic-release/git": "^10.0.0",
		"@storybook/storybook-deployer": "^2.8.0",
		"@web/test-runner": "^0.13.18",
		"@web/test-runner-selenium": "^0.5.2",
		"eslint": "^7.32.0",
		"husky": "^7.0.0",
		"semantic-release": "^18.0.0",
		"sinon": "^11.1.0",
		"typescript": "^4.4.0"
	}
}
