{
	"env": {
		"mocha": true
	},
	"plugins": ["mocha"],
	"parserOptions": {
		"ecmaVersion": 8
	},
	"globals": {
		"a11ySuite": false,
		"assert": false,
		"chai": true,
		"expect": false,
		"fixture": false,
		"flush": false,
		"MockInteractions": false,
		"sinon": true,
		"WCT": false
	},
	"rules": {
		"no-unused-expressions": "off"
	}
}
